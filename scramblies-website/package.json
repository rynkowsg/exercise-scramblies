{
  "name": "scramblies-website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "shadow:clj": "clj -M:dev:test:shadow-cljs",
    "shadow:npm": "shadow-cljs",
    "build:watch": "yarn shadow:clj watch client",
    "build:release": "yarn shadow:clj release client",
    "build:report": "yarn shadow:clj run shadow.cljs.build-report client target/build-report.html",
    "run-release": "yarn build:release; web-server -r ./resources/public"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "highlight.js": "11.1.0",
    "shadow-cljs": "2.15.10",
    "web-server-cli": "2.0.2"
  }
}
